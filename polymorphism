

class India:
    def capital(self):
        print("New Delhi is the capital of india")
    def language(self):
        print("Hindi is widely spoken")

class USA:
    def capital(self):
        print("Washington D.C is the capital of USA")
    def language(self):
        print("English is widely spoken")
obj1 = India()
obj2 = USA()
for i in (obj1, obj2):
    i.capital() #obj1.capital
    i.language() #obj1.language

obj1.capital()
obj1.language()
obj2.capital()
